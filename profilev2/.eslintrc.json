{
  "env": {

    "browser": true,

    "es2021": true

  },


  "extends": ["next/babel","next/core-web-vitals"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"

  },

  "plugins": ["react"],

  "rules": {

    "semi": ["error", "always"],

    "quotes": ["error", "double"],

    "indent": ["error", 2]

  }
  
}
